{
  "name": "webapp-main",
  "version": "1.0.0",
  "description": "Campus Diaries Main WebApp",
  "author": "",
  "main": "bin/server.js",
  "scripts": {
    "eslint": "node_modules/eslint/bin/eslint.js src/",
    "test": "UNIT_TESTING=true mocha --compilers png: --require ./test/mocha_config.js --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "build": "better-npm-run build",
    "watch-client": "better-npm-run watch-client",
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-dev": "better-npm-run start-dev",
    "start-dev-debug": "better-npm-run start-dev-debug",
    "start-prod": "better-npm-run start-prod",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "dev-debug": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev-debug\"",
    "ci-test": "UNIT_TESTING=true mocha --compilers png: --require ./test/mocha_config.js --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\" -R xunit | grep \"<\" > test-reports.xml",
    "build:dll": "webpack --config ./webpack/config.dll.js"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-dev-debug": {
      "command": "node-debug --no-preload ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_FETCH_TOKEN": true
      }
    },
    "watch-client": {
      "command": "node ./webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --display-chunks --display-modules --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [],
  "license": "ISC",
